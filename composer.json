{
    "name": "scottybo/linkedin-api-client-v2",
    "type": "library",
    "description": "LinkedIn API v2 client. Handles OAuth, CSRF protection. Easy to implement and extend. This is a standalone library for any composer project.",
    "keywords": ["LinkedIn", "OAuth", "API", "Client", "SDK", "LinkedIn v2"],
    "homepage": "https://github.com/scottybo/LinkedIn-API-client-v2",
    "license": "MIT",
    "authors": [
        {
            "name": "Scott Bowler",
            "email": "scott.bowler@dcsworldwide.net"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5",
        "php-http/guzzle5-adapter": "^1.0",
        "guzzlehttp/psr7": "^1.2",
        "mockery/mockery": "^0.9",
        "illuminate/support": "^5.0"
    },
    "autoload": {
        "psr-4": { "Scottybo\\LinkedIn2\\": "src/" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
